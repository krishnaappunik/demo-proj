{
  "name": "demo-proj",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "react-scripts test",
    "test:all": "jest --testPathPattern=\"**(/*)?(test|spec)?.(js|jsx)\" && echo 'All files tested.'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.5",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.2.2",
    "@testing-library/user-event": "^13.5.0",
    "jest": "^27.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [],
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.(js|jsx|ts|tsx)",
      "**/?(*.)+(spec|test).(js|jsx|ts|tsx)",
      "**/__mocks__/**/"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "babel-jest",
      "^.+\\.css$": "jest-transform-css",
      "^.+\\.less$": "jest-transform-css",
      "^.+\\.sass$": "jest-transform-css",
      "^.+\\.scss$": "identity-obj-proxy",
      "^.+\\.ts$": "ts-jest",
      "^.+\\.tsx$": "ts-jest",
      "^.+\\.js": "jsextract",
      "^.+\\.css": "css-extract",
      "^.+\\.less": "less-extract",
      "^.+\\.sass": "sass-extract",
      "^.+\\.scss": "sass-extract",
      "^.+\\.json$": "jest-extended",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|wav)": "file-loader"
    },
    "snapshotSerializers": [
      "jest-extended"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@?(react|react-native|raf|jest-)/)",
      "node_modules/(?!(jest-)?@?(react|react-native|raf|jest-)/react/)",
      "dist/",
      "<rootDir>/node_modules/",
      "<rootDir>/node_modules/react/dist/",
      "<rootDir>/node_modules/react-dom/dist/",
      "<rootDir>/node_modules/babel-loader/",
      "<rootDir>/node_modules/css-loader/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "maxWorkers": "75%",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/node_modules/react/dist/",
      "<rootDir>/node_modules/react-dom/dist/",
      "<rootDir>/node_modules/babel-loader/",
      "<rootDir>/node_modules/css-loader/"
    ],
    "testResultsProcessor": "",
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "check-leaks": {
      "enabled": true
    },
    "errorOnDeprecated": true
  },
  "devDependencies": {
    "@babel/runtime": "^7.11.8",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.0.0",
    "css-loader": "^6.7.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.0.1",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-environment-jsdom": "^27.5.1",
    "jest-extended": "^3.2.0",
    "jest-transform-css": "^6.0.1",
    "less-loader": "^10.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^27.1.5",
    "typescript": "^5.9.3",
    "webpack": "^5.77.0"
  }
}
